{
  "name": "n8n-nodes-excel-crud",
  "version": "1.0.0",
  "description": "n8n node for Excel file CRUD operations",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "excel",
    "crud",
    "spreadsheet"
  ],
  "license": "MIT",
  "homepage": "https://github.com/code4Copilot/n8n-nodes-excel-crud",
  "author": {
    "name": "Hueyan Chen",
    "email": "hueyan.chen@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code4Copilot/n8n-nodes-excel-crud.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint \"nodes/**/*.ts\" --ignore-pattern \"**/*.test.ts\"",
    "lintfix": "eslint \"nodes/**/*.ts\" --ignore-pattern \"**/*.test.ts\" --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/ExcelCrud/ExcelCrud.node.js"
    ]
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^5.0.1",
    "jest": "^29.5.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "exceljs": "^4.4.0"
  },
  "overrides": {
    "form-data": "^4.0.4"
  }
}
